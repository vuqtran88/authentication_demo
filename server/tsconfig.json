{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["node", "express"],
    "outDir": "./dist",
  },
  "include": ["src/**/*", "types/**/*"],
  "exclude": ["node_modules"]
}